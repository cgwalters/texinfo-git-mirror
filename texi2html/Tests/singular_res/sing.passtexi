../singular_texi/./d2t_singular/finvar_lib.tex(,632) @node evaluate_reynolds, invariant_basis, partial_molien, finvar_lib
../singular_texi/./d2t_singular/finvar_lib.tex(,633) @subsubsection evaluate_reynolds
