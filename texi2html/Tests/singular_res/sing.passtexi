../singular_texi/./d2t_singular/ntsolve_lib.tex(,107) ideal a = 2,3,4;
../singular_texi/./d2t_singular/ntsolve_lib.tex(,108) intvec e = 20,10;
../singular_texi/./d2t_singular/ntsolve_lib.tex(,109) ideal l = triMNewton(i,a,e);
